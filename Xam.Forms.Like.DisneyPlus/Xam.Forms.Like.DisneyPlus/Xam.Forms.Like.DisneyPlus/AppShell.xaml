       xmlns:disneyPlus="clr-namespace:Xam.Forms.Like.DisneyPlus;assembly=Xam.Forms.Like.DisneyPlus"
       x:Class="Xam.Forms.Like.DisneyPlus.AppShell">
    <TabBar>
        <Tab Title="Home">
            <ShellContent ContentTemplate="{DataTemplate disneyPlus:MainPage}" />
        </Tab>
        
        <Tab Title="Search">
            <ShellContent ContentTemplate="{DataTemplate disneyPlus:MainPage}" />
        </Tab>
        
        <Tab Title="Download">
            <ShellContent ContentTemplate="{DataTemplate disneyPlus:MainPage}" />
        </Tab>

        <Tab Title="Profile">
            <ShellContent ContentTemplate="{DataTemplate disneyPlus:MainPage}" />
        </Tab>

    </TabBar>
</Shell>